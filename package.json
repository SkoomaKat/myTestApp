{
  "name": "mytextapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": ["./__mocks__/@react-native-async-storage/async-storage.js"],
    "testRegex": "(/tst/.*|(\\.|/)(test|spec))\\.(ts|js)$",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@maps/(.*)$": "<rootDir>/assets/maps/$1",
      "^@images/(.*)$": "<rootDir>/assets/images/$1",
      "^@nodeData/(.*)$": "<rootDir>/assets/nodeData/$1"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-navigation/native": "^6.0.2",
    "async-storage": "^0.1.0",
    "expo": "~51.0.28",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-sharing": "~12.0.1",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.3",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.3.3"
  },
  "private": true
}
